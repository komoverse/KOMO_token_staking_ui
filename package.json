{
  "name": "komo",
  "version": "1.0.0",
  "author": "komo Team",
  "license": "MIT",
  "description": "Komo Token Staking Project",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-dashboard-material-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-material-ui.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start"
  },
  "dependencies": {
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@fortawesome/fontawesome-free": "5.15.2",
    "@material-ui/core": "4.11.3",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.57",
    "@nfteyez/sol-rayz": "^0.8.0",
    "@project-serum/anchor": "^0.18.0",
    "@project-serum/common": "0.0.1-beta.3",
    "@project-serum/sol-wallet-adapter": "^0.2.6",
    "@project-serum/tokens": "^0.0.7",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-registry": "^0.2.512",
    "@solana/wallet-adapter-base": "^0.7.1",
    "@solana/wallet-adapter-react": "^0.13.1",
    "@solana/wallet-adapter-react-ui": "^0.6.1",
    "@solana/wallet-adapter-wallets": "^0.11.3",
    "@solana/web3.js": "^1.31.0",
    "apexcharts": "^3.30.0",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "chart.js": "2.9.4",
    "coingecko-api": "^1.0.10",
    "dotenv": "^10.0.0",
    "nouislider": "14.6.3",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-apexcharts": "^1.3.9",
    "react-chartjs-2": "2.11.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-datetime": "3.0.4",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.1",
    "react-fast-marquee": "^1.2.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.2",
    "react-toastify": "^8.2.0",
    "react-tradingview-widget": "^1.3.2",
    "redux-debounced": "^0.5.0",
    "redux-thunk": "^2.4.0",
    "spinners-react": "^1.0.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-vars": "off",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "optionalDependencies": {
    "typescript": "4.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "7.20.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-unused-imports": "1.1.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8"
  }
}
